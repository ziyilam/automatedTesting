package newpackage;
import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

import newpackage.PG4;
public class PG5 {

	public static void main(String[] args) {
		//PG5 myObj = new PG5();
		PG4 myObj2 = new PG4();
		myObj2.invokeBrowser("http://ms.scrpower99.com//Member//MemberLogin");
		WebDriver driver = new ChromeDriver();
		String myString = "";
		myString = driver.findElement(By.className("form-control")).getText();
		System.out.println("the Text is: " + myString);
	}

}
